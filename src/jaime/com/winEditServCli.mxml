			private function isNewValue(option:Boolean,value:String):Boolean
			{
				var isNew:Boolean=true
				var i:int=0
				if(option)//search in pasajeros
				{
					for	(i=0;i<this.acpasajeros.length;i++)
						if(acpasajeros.getItemAt(i).pax_name == value)
							return false
					
